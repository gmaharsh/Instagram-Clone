\");\n    const [image, setImage] = useState(\"\");\n    const [url, setUrl] = useState(\"\");\n\n    const [state, dispatch] = useStateValue();\n    console.log(\"State from post:-\", state.user._id)\n\n    useEffect(() => {\n        console.log(\"url:-\",url)\n        if (url) {\n            fetch(\"/createPost\", {\n                method: \"POST\",\n                headers: {\n                    \"Content-Type\": \"application/json\",\n                    \"Authorization\": \"Bearer \" + localStorage.getItem(\"jwt\")\n                },\n                body: JSON.stringify({\n                    caption,\n                    image: url,\n                })\n            }).then(res => res.json())\n                .then(data => {\n                    if (data) {\n                        console.log(data)\n                        history.push(\"/\")\n                    } else {\n                    }\n                }).catch(error => console.log(error))\n        }\n    },[url])\n    \n    const postDetails = (e) => {\n        e.preventDefault()\n        const formData = new FormData();\n        formData.append('file', image)\n        formData.append('upload_preset', 'instagram-clone-maharsh');\n        formData.append('cloud_name', \"instagram-clone-maharsh\");\n        // console.log(formData)\n\n        fetch(\"https://api.cloudinary.com/v1_1/instagram-clone-maharsh/image/upload\", {\n            method: \"POST\",\n            body:formData\n        }).then(res => res.json())\n            .then(data => {\n                // createPost(data.url)\n                setUrl(data.url)\n            }).catch((err) => {\n                console.log(err)\n            })\n        \n    }\n    \n    \n\n    return (\n        <div className=\"post\">\n            <h4>Post an Image</h4>\n            <div className=\"post__form\">\n            <form>\n                <input\n                    type=\"text\"\n                    placeholder=\"Caption\"\n                    value={caption}\n                    onChange={(e) => setCaption(e.target.value)}\n                        \n                />\n                <input\n                    type=\"file\"\n                    onChange={(e) => setImage(e.target.files[0])}\n                />\n                <button type=\"submit\" onClick={postDetails}>Add a post</button>\n            </form>\n            </div>\n        </div>\n    )\n}\n\nexport default Post\n","/Users/maharshgheewala/Desktop/apps/Instagram-Clone/client/src/reducers/userReducer.js",[],"/Users/maharshgheewala/Desktop/apps/Instagram-Clone/client/src/reducers/StateProvider.js",[],"/Users/maharshgheewala/Desktop/apps/Instagram-Clone/client/src/Components/Profile/UserProfile/UserProfile.js",["110","111"],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":28,"column":6,"nodeType":"118","endLine":28,"endColumn":8,"suggestions":"119"},{"ruleId":"120","severity":1,"message":"121","line":1,"column":17,"nodeType":"122","messageId":"123","endLine":1,"endColumn":27},{"ruleId":"120","severity":1,"message":"124","line":4,"column":8,"nodeType":"122","messageId":"123","endLine":4,"endColumn":19},{"ruleId":"125","severity":1,"message":"126","line":12,"column":12,"nodeType":"127","messageId":"128","endLine":12,"endColumn":14},{"ruleId":"125","severity":1,"message":"126","line":17,"column":12,"nodeType":"127","messageId":"128","endLine":17,"endColumn":14},{"ruleId":"120","severity":1,"message":"129","line":15,"column":19,"nodeType":"122","messageId":"123","endLine":15,"endColumn":27},{"ruleId":"120","severity":1,"message":"130","line":17,"column":12,"nodeType":"122","messageId":"123","endLine":17,"endColumn":19},{"ruleId":"120","severity":1,"message":"131","line":17,"column":21,"nodeType":"122","messageId":"123","endLine":17,"endColumn":31},{"ruleId":"132","severity":1,"message":"133","line":48,"column":34,"nodeType":"134","messageId":"128","endLine":48,"endColumn